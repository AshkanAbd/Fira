<?xml vestion="1.0"?>
<launch>
    <node pkg="data_mapping" type="data_mapping" name="data_mapping" respawn="true" output="screen">
        <param name="map_frame" type="string" value="/map"/>
        <param name="odom_frame" type="string" value="/odom"/>
        <param name="map_topic" type="string" value="/base_map"/>
        <param name="model_topic" type="string" value="/gazebo/model_states"/>
        <param name="hz" type="int" value="10"/>
        <param name="height" type="int" value="4"/>
        <param name="width" type="int" value="6"/>
        <param name="resolution" type="double" value="0.1"/>
        <param name="initial_x" type="double" value="-0.8"/>
        <param name="initial_y" type="double" value="-2"/>
    </node>
</launch>